# Proxy Configuration
# List of target URLs separated by |
TARGET_URLS=localhost:3000|localhost:3001|localhost:3002

# Timeout in milliseconds for health checks
TIMEOUT=5000

# Port for the proxy server
PORT=7777

# Host binding (0.0.0.0 for all interfaces)
HOST=0.0.0.0

# Log level (error, warn, info, debug)
LOG_LEVEL=info

# Health check interval in milliseconds (default: 30000 = 30s)
HEALTH_CHECK_INTERVAL=30000

# Maximum HTTP status code considered healthy (default: 499)
MAX_HEALTHY_STATUS=499

# Circuit breaker failure threshold (default: 3)
CIRCUIT_BREAKER_THRESHOLD=3
