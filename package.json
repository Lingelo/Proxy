{
  "name": "smart-http-proxy",
  "version": "1.0.0",
  "description": "Intelligent HTTP proxy with load balancing, health checks, and comprehensive monitoring",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "node -r ts-node/register --env-file=.env src/index.ts --trace-warnings",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts tests/**/*.ts",
    "format:check": "prettier --check src/**/*.ts tests/**/*.ts",
    "type-check": "tsc --noEmit",
    "quality": "yarn lint && yarn format:check && yarn type-check",
    "security:audit": "yarn audit --audit-level moderate",
    "security:fix": "yarn audit fix",
    "docker:build": "docker build -t smart-proxy .",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose up"
  },
  "keywords": [
    "proxy",
    "load-balancer",
    "health-check",
    "monitoring",
    "typescript",
    "docker"
  ],
  "author": "Smart Proxy Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lingelo/Proxy"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "http-proxy": "^1.18.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.15",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.13.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
