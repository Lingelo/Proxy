{
  "name": "smart-http-proxy",
  "version": "1.0.0",
  "description": "Intelligent HTTP proxy with load balancing, health checks, and comprehensive monitoring",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "node -r ts-node/register --env-file=.env src/index.ts --trace-warnings",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t smart-proxy .",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose up"
  },
  "keywords": [
    "proxy",
    "load-balancer",
    "health-check",
    "monitoring",
    "typescript",
    "docker"
  ],
  "author": "Smart Proxy Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lingelo/Proxy"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "http-proxy": "^1.18.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.15",
    "@types/node": "^22.13.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
